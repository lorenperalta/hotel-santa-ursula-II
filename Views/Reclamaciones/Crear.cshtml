@model hotel_santa_ursula_II.Models.Reclamaciones
@{
 
}

<div class="text-center">
    <h6 class="display-4">reclamos</h6>
    <form asp-controller="Reclamaciones" asp-action="Registrar">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

        <div class="form-group text-left">
            <label asp-for="Dni" class="control-label"></label>
            <input asp-for="Dni" class="form-control rounded-pill">
            <span asp-validation-for="Dni" class="text-danger"></span>
        </div><br>

        <div class="form-group text-left">
            <label asp-for="Nombres" class="control-label"></label>
            <input asp-for="Nombres" class="form-control rounded-pill">
            <span asp-validation-for="Nombres" class="text-danger"></span>
        </div><br>
        <div class="form-group text-left">
            <label asp-for="Apellido" class="control-label"></label>
            <input asp-for="Apellido" class="form-control rounded-pill">
            <span asp-validation-for="Apellido" class="text-danger"></span>
        </div><br>
        <div class="form-group text-left">
            <label asp-for="N_Celular" class="control-label"></label>
            <input asp-for="N_Celular" class="form-control rounded-pill">
            <span asp-validation-for="N_Celular" class="text-danger"></span>
        </div><br>
        <div class="form-group text-left">
            <label asp-for="Correo" class="control-label"></label>
            <input asp-for="Correo" class="form-control rounded-pill">
            <span asp-validation-for="Correo" class="text-danger"></span>
        </div><br>
        <div class="form-group text-left">
            <label asp-for="Reclamo" class="control-label"></label>
            <input asp-for="Reclamo" class="form-control rounded-pill">
            <span asp-validation-for="Reclamo" class="text-danger"></span>
        </div><br>

        <div class="form-group">
            <button class="btn btn-primary btn-block rounded-pill">Crear Nuevo</button>
        </div><br>


        
    </form>
</div>
